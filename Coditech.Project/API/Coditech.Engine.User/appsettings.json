{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "CoditechDatabase": ""
  },
  "appsettings": {
    "ValidateRequestAPI": "false",
    "CoditechApiDomainName": "",
    "CoditechApiDomainKey": "",
    //To specify Coditech API request timeout in milliseconds
    "ApiRequestTimeout": "10000000",
    "MinifiedJsonResponseFromAPI": "false",
    "CoditechApiUriItemSeparator": ",",
    "CoditechCommaReplacer": "^",
    "CoditechApiUriKeyValueSeparator": "~",
    "EnableFileLogging": "1",
    "ResetPasswordExpriedTimeInMinute": "5"
  },
  "AllowedHosts": "*"
}
